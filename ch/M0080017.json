	"category": [
		"行政",
		"農業部",
		"農業科技園區目"
	],
	"LawModifiedDate": "20181210",
	"histories": [
		"一百零七年十二月十日行政院農業委員會屏東農業生物技術園區籌備處農生園籌二字第 1074017539A 號令訂定發布全文 6 條；並自發布日施行"
	],
	"articles": [
		{
			"number": 100,
			"content": [
				{
					"text": "本標準依規費法第十條第一項規定訂定之。"
				}
			]
		},
		{
			"number": 200,
			"content": [
				{
					"text": "建造執照及雜項執照按建築物造價或雜項工作物造價收取千分之一之規費。變更設計者，應按變更部分收取千分之一之規費。"
				}
			]
		},
		{
			"number": 300,
			"content": [
				{
					"text": "使用執照收取規費新臺幣二百元。"
				}
			]
		},
		{
			"number": 400,
			"content": [
				{
					"text": "拆除執照免費發給。"
				}
			]
		},
		{
			"number": 500,
			"content": [
				{
					"text": "建築執照遺失補發收取規費新臺幣二百元。"
				}
			]
		},
		{
			"number": 600,
			"content": [
				{
					"text": "本標準自發布日施行。"
				}
			]
		}
	]
}