{
	"法規性質": "命令",
	"法規名稱": "英文戶籍謄本規費收費標準",
	"法規網址": "http://law.moj.gov.tw/LawClass/LawOldVer.aspx?PCODE=D0030025&LNNDATE=20050128&LSER=001",
	"法規類別": "組改前廢止法規＞內政法規",
	"異動日期": "20050128",
	"法規內容": [
		{
			"條號": "第 1 條",
			"條文內容": "本標準依戶籍法第十二條及規費法第十條規定訂定之。"
		},
		{
			"條號": "第 2 條",
			"條文內容": "戶政事務所核發英文戶籍謄本之收費數額，每張新臺幣一百五十元。"
		},
		{
			"條號": "第 3 條",
			"條文內容": "本標準自發布日施行。"
		}
	]
}