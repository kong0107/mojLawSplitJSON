{
	"法規性質": "命令",
	"法規名稱": "非正規教育課程認可規費收費標準",
	"法規網址": "http://law.moj.gov.tw/LawClass/LawOldVer.aspx?PCODE=H0080070&LNNDATE=20060315&LSER=001",
	"法規類別": "行政＞教育部＞終身教育目",
	"異動日期": "20060315",
	"法規內容": [
		{
			"條號": "第 1 條",
			"條文內容": "本標準依規費法第十條第一項規定訂定之。"
		},
		{
			"條號": "第 2 條",
			"條文內容": "申請非正規教育課程認可者，以學分數為單位，每一學分收費新臺幣二千\r\n元。"
		},
		{
			"條號": "第 3 條",
			"條文內容": "課程未通過認可者，已繳之認可規費不予退還。"
		},
		{
			"條號": "第 4 條",
			"條文內容": "本標準自發布日施行。"
		}
	]
}